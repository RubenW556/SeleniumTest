node {
    stage "Create build output"
    checkout scm
    bat "dir"
    bat "path"
    bat "python --version"
    bat "pip --version"
    bat "python -m venv ./venv"
    bat "call ./venv/Scripts/activate.bat"
    bat "pip install -r ./requirements.txt"
    bat "pytest --junitxml report.xml"
    junit "test-results.xml"
}